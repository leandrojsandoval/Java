package parcial.biblioteca;

public class Ejemplar {

	private int id;
	private Libro libro;
	private boolean estaDisponible;

	public Ejemplar(int id, Libro libro) {
		super();
		this.id = id;
		this.libro = libro;
		this.estaDisponible = true;
	}

	public boolean fuePrestado() {
		return estaDisponible == false;
	}

}
