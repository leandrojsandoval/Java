package parcial.productos;

public class Usuario {
	
	private int id;
	private String nombre;
	private String apellido;
	private Cuenta cuenta;
	
	public Usuario(int id, String nombre, String apellido, Cuenta cuenta) {
		this.id = id;
		this.nombre = nombre;
		this.apellido = apellido;
		this.cuenta = cuenta;
	}
}
