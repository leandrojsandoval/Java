package edu.unlam.paradigmas.entradasalida.ej03;

public class Sumo {

	private double peso;
	private double altura;
	
	public Sumo(double peso, double altura) {
		this.peso = peso;
		this.altura = altura;
	}

	public double getPeso() {
		return peso;
	}

	public double getAltura() {
		return altura;
	}
	
}
